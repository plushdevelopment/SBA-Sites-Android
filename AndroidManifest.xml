<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
	package="com.sbasite.sbasites" android:versionName="@string/app_version"
	android:versionCode="1">
	<uses-permission android:name="android.permission.INTERNET" />
	<uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
	<uses-sdk android:minSdkVersion="7" android:targetSdkVersion="11"></uses-sdk>
	<uses-permission android:name="android.permission.ACCESS_FINE_LOCATION"></uses-permission>
	<application android:name="SBASitesApplication"
		android:label="@string/app_name" android:icon="@drawable/icon"
		android:theme="@style/Theme.OTweet">
		<meta-data android:name="AA_DB_NAME" android:value="Application.db" />
		<meta-data android:name="AA_DB_VERSION" android:value="1" />
		<uses-library android:required="true" android:name="com.google.android.maps"></uses-library>
		<activity android:label="@string/app_name" android:name=".SBAMapActivity">
			<intent-filter>
				<action android:name="android.intent.action.MAIN" />
				<category android:name="android.intent.category.LAUNCHER" />
			</intent-filter>
		</activity>
		<activity android:label="@string/instructions" android:name=".Instructions">
		</activity>
		<activity android:label="@string/layers" android:name=".Layers">
		</activity>
		<activity android:label="@string/list_view" android:name=".SiteListActivity">
		</activity>
		<activity android:label="@string/site_detail_activity"
			android:name=".SiteDetailActivity">
		</activity>
		<activity android:label="@string/site_name_id" android:name=".SearchSiteNameAddress">
		</activity>
		<activity android:label="@string/coordinates" android:name=".SearchCoordinates">
		</activity>
		<activity android:label="@string/address" android:name=".SearchAddress">
		</activity>
		<activity android:label="@string/about_sba_communications"
			android:name=".AboutSbaCommunications">
		</activity>
		<activity android:label="Search" android:name=".SearchListActivity"></activity>
		<activity android:label="Site Image" android:name=".Activity.SiteImageViewActivity"></activity>
	</application>
</manifest>